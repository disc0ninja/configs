!! Colorscheme

! special
URxvt.foreground: #93a1a1
URxvt.background: [97]#141c21
URxvt.depth: 32
URxvt.cursorColor: #afbfbf

!! fake transparency
!URxvt*inheritPixmap: true
!URxvt*tranparent: true
!URxvt*shading: 15

! black
URxvt.color0: #263640
URxvt.color8: #4a697d

! red
URxvt.color1: #d12f2c
URxvt.color9: #fa3935

! green
URxvt.color2: #819400
URxvt.color10: #a4bd00

! yellow
URxvt.color3: #b08500
URxvt.color11: #d9a400

! blue
URxvt.color4: #2587cc
URxvt.color12: #2ca2f5

! magenta
URxvt.color5: #696ebf
URxvt.color13: #8086e8

! cyan
URxvt.color6: #289c93
URxvt.color14: #33c5ba

! white
URxvt.color7: #bfbaac
URxvt.color15: #fdf6e3

!! fonts
!! URxvt Appearance
! need a nerd font backup for full glyph support in urxvt
URxvt.font: xft:Hack-Regular:size:10,xft:Inconsolata Nerd Font Mono:size=11
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.iso14755: false

! Disable ctl-alt-c & ctl-alt-v
! and enable copy and paste from ctl+shift+[c|v]
URxvt.keysym.C-M-c: builtin-string:
URxvt.keysym.C-M-v: builtin-string:
URxvt.keysym.C-S-0x43: eval:selection_to_clipboard
URxvt.keysym.C-S-0x56:  eval:paste_clipboard 

!-*- Perl extensions -*-
URxvt.perl-ext-common: default
URxvt.url-launcher:   /usr/bin/firefox
URxvt.underlineURLs:  True


